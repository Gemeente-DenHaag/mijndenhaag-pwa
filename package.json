{
  "name": "mijndenhaag-pwa",
  "description": "Progressive Web App based on React and Gatsby for the MijnDenHaag personal environment.",
  "version": "0.1.0",
  "author": "Municipality of The Hague",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop --verbose",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "generate:specs": "run-s generate:specs:*",
    "generate:specs:zaak": "npx openapi-typescript https://raw.githubusercontent.com/VNG-Realisatie/zaken-api/master/src/openapi.yaml --output src/types/zaak_schema.ts",
    "generate:specs:catalogi": "npx openapi-typescript https://raw.githubusercontent.com/VNG-Realisatie/catalogi-api/master/src/openapi.yaml  --output src/types/catalogi_schema.ts"
  },
  "dependencies": {
    "@gemeente-denhaag/denhaag-component-library": "^0.1.9",
    "acorn": "^8.0.4",
    "acorn-jsx": "^5.3.1",
    "gatsby": "^3.1.1",
    "gatsby-plugin-manifest": "^3.1.0",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-offline": "^4.1.0",
    "gatsby-plugin-typescript": "^3.1.0",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "got": "^11.8.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tsconfig-paths-webpack-plugin": "3.5.1"
  },
  "devDependencies": {
    "@types/node": "14.14.35",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "babel-plugin-root-import": "6.6.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^8.1.0",
    "gatsby-plugin-ts-config": "^1.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "typescript": "4.2.3"
  }
}
